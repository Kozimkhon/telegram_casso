{
  "name": "telegram_casso",
  "version": "1.0.0",
  "description": "Telegram UserBot + AdminBot system for channel management and message forwarding",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "jest",
    "test:e2e": "jest test/__tests__/e2e --runInBand",
    "test:e2e:watch": "jest test/__tests__/e2e --watch",
    "test:coverage": "jest --coverage",
    "db:init": "node src/db/init.js",
    "add-session": "node scripts/addSession.js",
    "list-sessions": "node scripts/listSessions.js",
    "migrate:typeorm": "node scripts/migrate-to-typeorm.js",
    "typeorm:quickstart": "node typeorm-quick-start.js",
    "typeorm:integration": "node integration-example.js",
    "build": "mkdir -p dist && cp -r src/* dist/",
    "start:prod": "NODE_ENV=production node src/index.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "userbot",
    "channel",
    "forwarding",
    "telegram",
    "telegraf"
  ],
  "author": "Developer",
  "license": "MIT",
  "dependencies": {
    "big-integer": "^1.6.52",
    "dotenv": "^17.2.3",
    "input": "^1.0.1",
    "inquirer": "^12.11.1",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "telegraf": "^4.15.6",
    "telegram": "^2.26.22",
    "typeorm": "^0.3.27",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@jest/globals": "^29.7.0",
    "@types/node": "^24.10.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "esbuild": "^0.27.0",
    "esbuild-register": "^3.6.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "jest-node-exports-resolver": "^1.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "test": "test"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
